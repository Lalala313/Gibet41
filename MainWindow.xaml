<Window x:Class="Гибет41размер.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        WindowState="Maximized"
        WindowStyle="None"
        ResizeMode="NoResize"
        xmlns:local="clr-namespace:Гибет41размер"
        mc:Ignorable="d"
        Title="41 размер" Height="450" Width="800" FontFamily="Comic Sans MS" Icon="/Images/icon.ico" MinHeight="400" MinWidth="700">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid Background="#FFFFCC99" ></Grid>
        <Grid Grid.Row="2" Background="#FFFFCC99" ></Grid>
        <TextBlock Text="41 размер" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="40"></TextBlock>
        <Image Source="/Images/logo.png" HorizontalAlignment="Left" />
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="ButtonBack" HorizontalAlignment="Right" Click="ButtonBack_Click"></Button>
        
    </Grid>
</Window>
